/* {{ description }} */

#include <stdio.h>
{% for header in headers %}
#include <{{ header }}>
{% endfor %}

{% if define_constants %}
{% for const in constants %}
#define {{ const.name }} {{ const.value }}
{% endfor %}
{% endif %}

int {{ function_name }}({% for arg in args %}{{ arg.type }} {{ arg.name }}{{ ", " if not loop.last }}{% endfor %}) {
    return {{ return_value }};
}
